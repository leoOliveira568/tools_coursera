# Exercise 8
# Code snippet for adding and multiplying numbers
x = 6
y = 4
total = x * y + y
print("The total after multiplying and adding is:", total)

# Exercise 9
# Code snippet for converting hours to minutes
def convert_hours_to_minutes(hours):
    minutes = hours * 60
    return minutes

hours = 2
minutes = convert_hours_to_minutes(hours)
print(f"{hours} hours is equivalent to {minutes} minutes.")

